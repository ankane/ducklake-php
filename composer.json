{
    "name": "ankane/ducklake",
    "description": "DuckLake for PHP",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ankane/ducklake-php/issues",
        "source": "https://github.com/ankane/ducklake-php"
    },
    "authors": [
        {
            "name": "Andrew Kane",
            "email": "andrew@ankane.org"
        }
